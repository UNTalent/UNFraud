{% set isReport = isReport ?? false %}
<table>
    {% for element in elements %}
        {% set domain = isReport ? element.domain : element %}
        <tr>
            <td><a href="{{ path('app_domain_check', {host: domain.host}) }}">{{ domain }}</a></td>
            <td>{{ domain.analysis.rating }}</td>
            <td class="right"><span
                        class="badge alert {{ domain.analysis.rating.cssClass }}" {{ domain.analysis.rating }}</td>
        </tr>
    {% endfor %}
</table>
